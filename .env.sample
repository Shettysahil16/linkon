# .env.sample for Frontend and Backend Environment Variables

# Frontend Environment Variables (for Cloud or any other frontend-specific config)
REACT_APP_CLOUD_NAME=your-cloud-name  # Replace with your cloud name for the frontend (e.g., cloud storage or CDN)

# Backend Environment Variables

# MongoDB URI for connecting to the database
MONGODB_URI=mongodb://localhost:27017/your-database-name  # Replace with your actual MongoDB URI

# JWT Secret Token for authentication
JWT_SECRET_TOKEN=your-jwt-secret-token  # Replace with your actual JWT secret token

# Frontend URL for CORS configuration (e.g., for handling CORS between frontend and backend)
FRONTEND_URL=http://localhost:3000  # Replace with your actual frontend URL for CORS (e.g., production or local URL)
